local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("UZAMA กังกัง HUB", "DarkTheme")
local Tab = Window:NewTab("Farm")
local Section = Tab:NewSection("Farm Select")

local Moblist = {}
local Mob = nil
for i,v in pairs(game:GetService("Workspace").Enemy:GetChildren()) do
   table.insert(Moblist, v.Name)
end

Section:NewDropdown("select mob", " ", Moblist, function(currentOption2)
    Mob = currentOption2
end)

Section:NewToggle("Farm", "ToggleInfo", function(state)
 punpun = state
end)
spawn(function()
while wait() do
    if punpun then
        pcall(function()
    
local char = game.Players.LocalPlayer.Character.HumanoidRootPart
for i,v in pairs(game:GetService("Workspace").Enemy:FindFirstChild(Mob):GetDescendants()) do 
    if v.Name == 'HumanoidRootPart' and v.Parent.Humanoid.Health > 0 then
        char.CFrame = v.CFrame * CFrame.new(0,0,3)
        game:GetService'VirtualUser':CaptureController()
        game:GetService'VirtualUser':Button1Down(Vector2.new(686, 352))
        game:GetService("Players").LocalPlayer.Backpack.Haki.Haki:FireServer("Haki")
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(Weapon))
                
                end
             end
    end)
    end
end
end)
local Section = Tab:NewSection("Weapon Select")
local Weaponlist = {}
local Weapon = nil

for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
    table.insert(Weaponlist,v.Name)
end

local dropdown = Section:NewDropdown("Select Weapon"," ", Weaponlist , function(adw)
 Weapon = adw
end)

Section:NewToggle("Auto Equip", " ", function(a)
AutoEquiped = a
end)


spawn(function()
while wait() do
if AutoEquiped then
pcall(function()
game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(Weapon))
end)
end
end
end)

local Section = Tab:NewSection("Inv Change")

local punlist = {}
local punew = nil
for i,v in pairs(game:GetService("Players").LocalPlayer.Inv:GetChildren()) do
table.insert(punlist, v.Name)
end


Section:NewDropdown("Select", "", punlist , function(pledf)
    punew = pledf
    if punew then
        pcall(function()
    game:GetService("ReplicatedStorage").Remote.Inv:FireServer(punew)
end)
end
end)


local Tab = Window:NewTab("Skill")
local Section = Tab:NewSection("Must Equiq DevilFruit")
local Section = Tab:NewSection("Light")
Section:NewToggle("AutoFarmDF", "ToggleInfo", function(state)
    if state then
        _G.cdl = true
while _G.cdl do wait()
    pcall(function()
game.Players.LocalPlayer.Character.Light.Event:FireServer("BarrageLight")
game.Players.LocalPlayer.Character.Light.Event:FireServer("RainLight")
end)
end
    else
        _G.cdl = false
while _G.cdl do wait()
    pcall(function()
game.Players.LocalPlayer.Character.Light.Event:FireServer("BarrageLight")
game.Players.LocalPlayer.Character.Light.Event:FireServer("RainLight")
end)
end
    end
end)
