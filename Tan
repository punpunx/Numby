local Tab = Window:NewTab("MISC")
local Section = Tab:NewSection("Bring Fruit Frome Anther Backpack")
Section:NewToggle("Auto Bring Fruit", "ToggleInfo", function(asvsdf)
    autobring = asvsdf
end)

spawn(function()
    if autobring then
    while wait() do
for i,v in pairs(game:GetService("Players"):GetDescendants()) do
    if v.Name == '???' then
    v.Parent = game.Players.LocalPlayer.Backpack
    end
end
end
end
end)

local Section = Tab:NewSection("Bring Scroll")
Section:NewToggle("Auto Bring Scroll", "ToggleInfo", function(asvsdfe)
    autobringe = asvsdfe
end)

spawn(function()
    if autobringe then
    while wait() do
for i,v in pairs(game:GetService("Players"):GetDescendants()) do
    if v.Name == 'Scroll' then
    v.Parent = game.Players.LocalPlayer.Backpack
    end
end
end
end
end)



